{
  "name": "landingpage",
  "version": "1.0.0",
  "description": "Landingpage for the online marketplace application",
  "author": "huhn511",
  "private": true,
  "scripts": {
    "dev": "cross-env NODE_ENV=development nodemon server/index.js --watch server",
    "build": "nuxt build",
    "start": "cross-env NODE_ENV=production node server/index.js",
    "generate": "nuxt generate",
    "generate:gh-pages": "cross-env DEPLOY_ENV=GH_PAGES nuxt generate",
    "deploy:gh-pages": "push-dir --dir=dist --branch=gh-pages --cleanup",
    "deploy": "npm run generate:gh-pages && npm run deploy:gh-pages",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "lint-fix": "eslint --ext .js,.vue --ignore-path .gitignore . --fix"
  },
  "dependencies": {
    "@iota/area-codes": "^0.1.1",
    "@nuxtjs/axios": "^5.3.6",
    "@nuxtjs/dotenv": "^1.4.0",
    "babel-eslint": "^10.1.0",
    "cross-env": "^5.2.0",
    "element-ui": "^2.4.11",
    "express": "^4.17.3",
    "leaflet-control-geocoder": "^1.13.0",
    "nuxt": "^2.0.0",
    "nuxt-i18n": "^6.6.1",
    "nuxt-leaflet": "0.0.21",
    "vue-gallery": "^2.0.1",
    "vue-scroll-reveal": "^1.0.11",
    "vue-scrollto": "^2.19.1",
    "vue-solid-plugin": "^0.6.0"
  },
  "devDependencies": {
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^4.1.0",
    "eslint-plugin-nuxt": ">=0.4.2",
    "eslint-plugin-prettier": "^3.0.1",
    "@nuxtjs/eslint-config": "^1.0.1",
    "@nuxtjs/eslint-module": "^1.0.0",
    "@vue/test-utils": "^1.0.0-beta.27",
    "babel-eslint": "^10.0.1",
    "node-sass": "^4.13.1",
    "nodemon": "^1.18.9",
    "push-dir": "^0.4.1",
    "sass-loader": "^8.0.2"
  }
}
